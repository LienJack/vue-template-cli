{
  "name": "<%= projectName %>",
  "version": "<%= projectVersion %>",
  "description": "<%= projectDescription %>",
  "main": "index.js",
  "scripts": {
    "build-dll-dev": "cross-env NODE_ENV=development PROCESS_ENV='LOCAL' webpack --config webpack/dllConfig.js",
    "build-dll": "cross-env NODE_ENV=production PROCESS_ENV='PROD' webpack --config webpack/dllConfig.js",
    "dev": "cross-env PROCESS_ENV='LOCAL' NODE_ENV=development node ./webpack/devserver.js",
    "build-prod": "cross-env PROCESS_ENV='PROD' NODE_ENV=production node ./webpack/index.js",
    "build-dev": "cross-env PROCESS_ENV='DEV' NODE_ENV=production node ./webpack/index.js",
    "build-test": "cross-env PROCESS_ENV='TEST' NODE_ENV=production node ./webpack/index.js",
    "build-grey": "cross-env PROCESS_ENV='GREY' NODE_ENV=production node ./webpack/index.js",
    "build-pre": "cross-env PROCESS_ENV='PRE' NODE_ENV=production node ./webpack/index.js",
    "start": "cross-env PROCESS_ENV='LOCAL' NODE_ENV=development node ./webpack/devserver.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LienJack/vue-template-cli.git"
