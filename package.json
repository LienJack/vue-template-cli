{
  "name": "vue-template-cli",
  "version": "1.0.0",
  "description": "vue脚手架",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist ./dist/",
    "clean-dll": "rimraf src/static/dll/",
    "build-dll": "npm run clean-dll && webpack --config webpack/dllConfig.js  --progress",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.dev.js --progress --hot --inline",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js --progress --mode production"
  },
